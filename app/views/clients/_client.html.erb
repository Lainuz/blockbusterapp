<div id="<%= dom_id client %>">
  <tr>
    <th scope="row"><%= client.id %></th>
    <td><%= client.name %></td>
    <td><%= client.age %></td>
    <td><%= client.movies.last %></td>
    <td><%= link_to "Mostrar cliente", client, class:"btn btn-outline-success btn-sm" %></td>
    <td><%= link_to "Editar Cliente", edit_client_path(client), class:"btn btn-outline-warning btn-sm" %></td>
    <td><%= button_to "Eliminar cliente", client, method: :delete, class:"btn btn-danger btn-sm" %></td>
  </tr>
</div>
